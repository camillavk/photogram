<section id='form'>
  <%= form_for @post, html: {multipart: true} do |f| %>
    <%= f.label :image %>
    <%= f.file_field :image %>
    <%= f.label :caption %>
    <%= f.text_field :caption %>
    <%= f.label :price %>
    <%= f.number_field :price %>
    <%= f.submit 'Update Post' %>
  <% end %>
</section>
